    "workbox-window": "^6.5.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix --no-eslintrc --config .eslintrc.core.js"
    ],
    "*.scss": [
      "stylelint --fix",
      "prettier --write"
    ]
  }
}
